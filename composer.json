{
  "name": "swoft-fork/error",
  "type": "library",
  "version": "v2.1.0",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "error-handler",
    "exception"
  ],
  "description": "swoft error handle component",
  "homepage": "https://github.com/sky-up/swoft-error",
  "license": "Apache-2.0",
  "require": {
    "swoft-fork/stdlib": "~2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Error\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Error\\Unit\\": "test/unit",
      "SwoftTest\\Error\\Testing\\": "test/testing"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0",
    "swoft-fork/framework": "~2.1.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
